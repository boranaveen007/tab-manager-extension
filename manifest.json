{
    "manifest_version": 3,
    "name": "Tab Manager",
    "version": "1.0",
    "description": "A simple tab manager extension",
    "permissions": ["storage", "tabs"],
    "background": {
      "service_worker": "src/background.js"
    },
    "action": {
      "default_popup": "src/popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    }
  }